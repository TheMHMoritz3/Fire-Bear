<UserControl x:Class="WindowsClient.Widgets.Widgets.MessageAndUserWidget"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:WindowsClient.Widgets.Widgets"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <DockPanel>
        <ListBox x:Name="listBox" Height="300" DockPanel.Dock="Left" Width="100"/>
        <TreeView x:Name="treeView" Height="300" DockPanel.Dock="Right"/>

    </DockPanel>
</UserControl>
