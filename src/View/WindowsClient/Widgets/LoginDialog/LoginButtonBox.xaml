<UserControl x:Class="WindowsClient.Widgets.LoginDialog.LoginButtonBox"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:WindowsClient.Widgets.LoginDialog"
             mc:Ignorable="d" d:DesignHeight="20" d:DesignWidth="155">
    <Grid>
        <Button x:Name="LoginButton" Content="Login" HorizontalAlignment="Left" Width="75" Height="20" VerticalAlignment="Top"/>
        <Button x:Name="CancelButton" Content="Cancel" HorizontalAlignment="Right" VerticalAlignment="Top" Width="75"/>

    </Grid>
</UserControl>
